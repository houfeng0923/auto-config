  "peacock.affectTitleBar": true,
  "peacock.elementAdjustments": {
    "activityBar": "lighten"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.DS_Store": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.git/**": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/bower_components/**": true,
    "**/CVS": true,
    "**/node_modules/**": false,
    "**/tmp/**": true,
    "h": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true,
  },
  "window.zoomLevel": 0,
  "window.nativeTabs": true,
  "editor.fontFamily": "Hack, Menlo, Monaco, 'Courier New', monospace",
